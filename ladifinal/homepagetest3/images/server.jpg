    <path d="M180,55 Q195,55 195,65 Q195,75 190,80" stroke="#3498db" stroke-width="2" fill="none"/>
    <path d="M180,85 Q188,85 188,95 Q188,105 185,110" stroke="#f39c12" stroke-width="2" fill="none"/>
