<?xml version="1.0" encoding="UTF-8"?>
<svg width="320" height="240" viewBox="0 0 320 240" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="320" height="240" fill="#f8f9fa"/>
  
  <!-- Workstation -->
  <g transform="translate(40, 30)">
    <!-- Main case (larger) -->
    <rect x="0" y="0" width="100" height="150" rx="10" fill="#1a1a1a"/>
    <rect x="5" y="5" width="90" height="140" rx="8" fill="#2c3e50"/>
    
    <!-- Professional front panel -->
    <rect x="15" y="20" width="70" height="12" rx="3" fill="#34495e"/>
    <text x="50" y="30" font-family="Arial, sans-serif" font-size="8" font-weight="bold" text-anchor="middle" fill="white">WORKSTATION</text>
    
    <!-- Multiple drive bays -->
    <rect x="15" y="45" width="70" height="8" rx="2" fill="#555"/>
    <rect x="15" y="58" width="70" height="8" rx="2" fill="#555"/>
    <rect x="15" y="71" width="70" height="8" rx="2" fill="#555"/>
    <rect x="15" y="84" width="70" height="8" rx="2" fill="#555"/>
    
    <!-- Status LEDs -->
    <circle cx="25" cy="105" r="3" fill="#27ae60"/>
    <circle cx="35" cy="105" r="3" fill="#3498db"/>
    <circle cx="45" cy="105" r="3" fill="#e67e22"/>
    <circle cx="55" cy="105" r="3" fill="#9b59b6"/>
    
    <!-- Pro ports -->
    <rect x="20" y="120" width="8" height="6" rx="2" fill="#1a1a1a"/>
    <rect x="30" y="120" width="8" height="6" rx="2" fill="#1a1a1a"/>
    <rect x="40" y="120" width="8" height="6" rx="2" fill="#1a1a1a"/>
    <rect x="50" y="120" width="8" height="6" rx="2" fill="#1a1a1a"/>
    <rect x="60" y="120" width="8" height="6" rx="2" fill="#1a1a1a"/>
    
    <!-- Professional Monitor (larger) -->
    <rect x="120" y="10" width="140" height="100" rx="10" fill="#1a1a1a"/>
    <rect x="130" y="20" width="120" height="80" rx="6" fill="#000"/>
    
    <!-- CAD/Design screen -->
    <rect x="140" y="30" width="100" height="60" rx="4" fill="#2c3e50"/>
    
    <!-- 3D model visualization -->
    <polygon points="160,45 180,35 200,45 200,65 180,75 160,65" fill="#3498db"/>
    <polygon points="180,35 200,45 200,65 180,75" fill="#2980b9"/>
    <polygon points="160,45 180,35 180,75 160,65" fill="#5dade2"/>
    
    <!-- Grid lines -->
    <line x1="145" y1="40" x2="235" y2="40" stroke="#666" stroke-width="0.5"/>
    <line x1="145" y1="50" x2="235" y2="50" stroke="#666" stroke-width="0.5"/>
    <line x1="145" y1="60" x2="235" y2="60" stroke="#666" stroke-width="0.5"/>
    <line x1="145" y1="70" x2="235" y2="70" stroke="#666" stroke-width="0.5"/>
    <line x1="145" y1="80" x2="235" y2="80" stroke="#666" stroke-width="0.5"/>
    
    <!-- Professional stand -->
    <rect x="185" y="110" width="10" height="25" rx="3" fill="#34495e"/>
    <rect x="165" y="133" width="50" height="6" rx="3" fill="#34495e"/>
    
    <!-- Professional keyboard -->
    <rect x="125" y="155" width="130" height="25" rx="6" fill="#2c3e50"/>
    <rect x="130" y="160" width="120" height="15" rx="3" fill="#1a1a1a"/>
    
    <!-- Professional mouse -->
    <ellipse cx="270" cy="167" rx="10" ry="15" fill="#2c3e50"/>
  </g>
  
  <!-- Product info -->
  <text x="160" y="210" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#2c3e50">Workstation Pro</text>
  <text x="160" y="225" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#666">Xeon • Quadro RTX • 32GB ECC</text>
</svg>