<?xml version="1.0" encoding="UTF-8"?>
<svg width="320" height="240" viewBox="0 0 320 240" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="320" height="240" fill="#f8f9fa"/>
  
  <!-- Mini PC -->
  <g transform="translate(80, 60)">
    <!-- Main body (compact cube) -->
    <rect x="0" y="0" width="80" height="80" rx="12" fill="#2c3e50"/>
    <rect x="5" y="5" width="70" height="70" rx="10" fill="#34495e"/>
    
    <!-- Compact design elements -->
    <rect x="15" y="15" width="50" height="8" rx="3" fill="#ecf0f1"/>
    <text x="40" y="22" font-family="Arial, sans-serif" font-size="6" font-weight="bold" text-anchor="middle" fill="#2c3e50">MINI PC</text>
    
    <!-- Ventilation grilles -->
    <rect x="12" y="30" width="56" height="2" rx="1" fill="#555"/>
    <rect x="12" y="35" width="56" height="2" rx="1" fill="#555"/>
    <rect x="12" y="40" width="56" height="2" rx="1" fill="#555"/>
    <rect x="12" y="45" width="56" height="2" rx="1" fill="#555"/>
    
    <!-- Status LED -->
    <circle cx="20" cy="60" r="3" fill="#27ae60"/>
    <text x="30" y="64" font-family="Arial, sans-serif" font-size="5" fill="#ecf0f1">PWR</text>
    
    <!-- Compact ports -->
    <rect x="50" y="57" width="6" height="4" rx="1" fill="#1a1a1a"/>
    <rect x="58" y="57" width="6" height="4" rx="1" fill="#1a1a1a"/>
    <rect x="50" y="65" width="6" height="4" rx="1" fill="#1a1a1a"/>
    <rect x="58" y="65" width="6" height="4" rx="1" fill="#1a1a1a"/>
    
    <!-- Compact monitor -->
    <rect x="100" y="-10" width="100" height="70" rx="8" fill="#1a1a1a"/>
    <rect x="108" y="-2" width="84" height="54" rx="5" fill="#000"/>
    
    <!-- Desktop screen -->
    <rect x="115" y="5" width="70" height="40" rx="3" fill="#3498db"/>
    
    <!-- Desktop icons -->
    <rect x="120" y="10" width="8" height="8" rx="2" fill="#f39c12"/>
    <rect x="132" y="10" width="8" height="8" rx="2" fill="#e74c3c"/>
    <rect x="144" y="10" width="8" height="8" rx="2" fill="#2ecc71"/>
    <rect x="156" y="10" width="8" height="8" rx="2" fill="#9b59b6"/>
    
    <!-- Taskbar -->
    <rect x="118" y="38" width="66" height="5" rx="2" fill="#34495e"/>
    
    <!-- Stand -->
    <rect x="145" y="60" width="6" height="15" rx="2" fill="#34495e"/>
    <rect x="130" y="73" width="36" height="4" rx="2" fill="#34495e"/>
    
    <!-- Wireless symbols -->
    <path d="M90,95 Q95,90 100,95" stroke="#27ae60" stroke-width="2" fill="none"/>
    <path d="M85,100 Q95,85 105,100" stroke="#27ae60" stroke-width="1.5" fill="none"/>
    <circle cx="95" cy="102" r="2" fill="#27ae60"/>
  </g>
  
  <!-- Product info -->
  <text x="160" y="200" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#2c3e50">Mini PC Ultra</text>
  <text x="160" y="215" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#666">Intel Core • 16GB • 512GB NVMe</text>
  <text x="160" y="228" font-family="Arial, sans-serif" font-size="9" text-anchor="middle" fill="#27ae60">Tiết kiệm không gian</text>
</svg>