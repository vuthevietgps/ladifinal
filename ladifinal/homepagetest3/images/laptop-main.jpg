<?xml version="1.0" encoding="UTF-8"?>
<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="300" fill="#f8f9fa"/>
  
  <!-- Laptop outline -->
  <g transform="translate(50, 50)">
    <!-- Laptop base -->
    <rect x="0" y="120" width="300" height="20" rx="10" fill="#2c3e50"/>
    <rect x="10" y="100" width="280" height="20" rx="8" fill="#34495e"/>
    
    <!-- Laptop screen -->
    <rect x="40" y="20" width="220" height="140" rx="15" fill="#2c3e50"/>
    <rect x="50" y="30" width="200" height="120" rx="8" fill="#1a1a1a"/>
    
    <!-- Screen content -->
    <rect x="60" y="40" width="180" height="100" rx="5" fill="#0066cc"/>
    
    <!-- Gaming elements -->
    <circle cx="120" cy="70" r="15" fill="#ff6b6b"/>
    <circle cx="160" cy="70" r="15" fill="#4ecdc4"/>
    <circle cx="200" cy="70" r="15" fill="#ffe66d"/>
    
    <!-- Gaming text -->
    <text x="150" y="110" font-family="Arial, sans-serif" font-size="12" font-weight="bold" text-anchor="middle" fill="white">GAMING</text>
    <text x="150" y="125" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#ccc">ROG STRIX</text>
    
    <!-- Keyboard -->
    <rect x="60" y="110" width="180" height="8" rx="2" fill="#555"/>
    
    <!-- Laptop brand -->
    <circle cx="150" cy="85" r="3" fill="white" opacity="0.3"/>
  </g>
  
  <!-- Tech Store text -->
  <text x="200" y="260" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#2c3e50">Laptop Gaming ROG Strix</text>
  <text x="200" y="280" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#666">High Performance Gaming</text>
</svg>