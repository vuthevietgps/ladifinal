<?xml version="1.0" encoding="UTF-8"?>
<svg width="320" height="240" viewBox="0 0 320 240" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="320" height="240" fill="#f8f9fa"/>
  
  <!-- All-in-One PC -->
  <g transform="translate(40, 20)">
    <!-- Main display (large integrated screen) -->
    <rect x="0" y="0" width="200" height="140" rx="15" fill="#1a1a1a"/>
    <rect x="10" y="10" width="180" height="120" rx="12" fill="#000"/>
    
    <!-- Screen content -->
    <rect x="20" y="20" width="160" height="100" rx="8" fill="#ecf0f1"/>
    
    <!-- Desktop wallpaper -->
    <rect x="25" y="25" width="150" height="90" rx="6" fill="#3498db"/>
    
    <!-- Mountains landscape -->
    <polygon points="25,90 50,60 80,75 120,45 160,70 175,85 175,115 25,115" fill="#2c3e50"/>
    <polygon points="40,80 70,50 100,65 140,35 170,60 175,70 175,115 40,115" fill="#34495e"/>
    
    <!-- Sun -->
    <circle cx="150" cy="40" r="8" fill="#f1c40f"/>
    
    <!-- Clouds -->
    <ellipse cx="60" cy="35" rx="12" ry="6" fill="white" opacity="0.8"/>
    <ellipse cx="130" cy="30" rx="15" ry="8" fill="white" opacity="0.8"/>
    
    <!-- Desktop icons -->
    <rect x="30" y="30" width="12" height="12" rx="3" fill="#e74c3c"/>
    <rect x="30" y="46" width="12" height="12" rx="3" fill="#f39c12"/>
    <rect x="30" y="62" width="12" height="12" rx="3" fill="#2ecc71"/>
    <rect x="30" y="78" width="12" height="12" rx="3" fill="#9b59b6"/>
    
    <!-- Taskbar -->
    <rect x="25" y="105" width="150" height="8" rx="4" fill="#34495e"/>
    
    <!-- Camera -->
    <circle cx="100" cy="15" r="3" fill="#2c3e50"/>
    
    <!-- Integrated stand -->
    <rect x="85" y="140" width="30" height="20" rx="8" fill="#2c3e50"/>
    <rect x="60" y="158" width="80" height="8" rx="4" fill="#34495e"/>
    
    <!-- Wireless keyboard (sleek) -->
    <rect x="20" y="180" width="120" height="25" rx="8" fill="#ecf0f1"/>
    <rect x="25" y="185" width="110" height="15" rx="5" fill="#34495e"/>
    
    <!-- Keys -->
    <rect x="30" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="40" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="50" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="60" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="70" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="80" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="90" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="100" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="110" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="120" y="188" width="8" height="4" rx="1" fill="#2c3e50"/>
    
    <rect x="35" y="194" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="45" y="194" width="40" height="4" rx="1" fill="#2c3e50"/>
    <rect x="87" y="194" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="97" y="194" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="107" y="194" width="8" height="4" rx="1" fill="#2c3e50"/>
    <rect x="117" y="194" width="8" height="4" rx="1" fill="#2c3e50"/>
    
    <!-- Wireless mouse (compact) -->
    <ellipse cx="170" cy="192" rx="8" ry="12" fill="#ecf0f1"/>
    <ellipse cx="170" cy="192" rx="6" ry="10" fill="#34495e"/>
  </g>
  
  <!-- Product info -->
  <text x="160" y="220" font-family="Arial, sans-serif" font-size="14" font-weight="bold" text-anchor="middle" fill="#2c3e50">All-in-One PC</text>
  <text x="160" y="235" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#666">27" 4K • Core i7 • 16GB • Touch</text>
</svg>